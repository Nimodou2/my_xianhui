+
+
+    public AvatarImageView(Context context, AttributeSet attrs) {
+        super(context, attrs);
+        this.mContext = context;
+        //在编辑模式下不需要进行初始化
+        if (!this.isInEditMode()) {
+            //初始化文件相关
