+        public int compare(Object o1, Object o2) {
+            Cards lhs=(Cards) o1;
+            Cards rhs=(Cards) o2;
+            if (lhs.getItem_id() > rhs.getItem_id())
+            {
+                return 1;
+            }
+            if (lhs.getItem_id() == rhs.getItem_id())
+            {
+                return 0;
+            }
+            if (lhs.getItem_id() < rhs.getItem_id())
+            {
+                return -1;
+            }
+            return 0;
+        }
+    }
+}
