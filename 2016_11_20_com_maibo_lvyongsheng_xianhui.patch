+
+        tv_cash = (TextView) findViewById(R.id.tv_cash);
+        tv_mcash = (TextView) findViewById(R.id.tv_mcash);
+        tv_project = (TextView) findViewById(R.id.tv_project);
+        tv_mproject = (TextView) findViewById(R.id.tv_mproject);
