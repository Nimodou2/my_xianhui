+    public void doSomething(){
+        //权限已获得
+            startActivity(new Intent(getActivity(),MySelfInformationActivity.class));
+    }
+    @PermissionFail(requestCode = 50)
+    public void doFailSomething(){
+        App.showToast(getContext(),"请设置权限");
+    }
