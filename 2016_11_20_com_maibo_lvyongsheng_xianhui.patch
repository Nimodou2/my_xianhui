+
+    //裁剪完照片的回调
+    private AfterCropListener afterCropListener;
+    private String btnClickedColor;
+
+    public AvatarImageView(Context context) {
+        super(context);
+        this.mContext = context;
+        //在编辑模式下不需要进行初始化
