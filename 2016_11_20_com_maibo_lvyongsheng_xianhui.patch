+
+    public AvatarImageView(Context context, AttributeSet attrs, int defStyleAttr) {
+        super(context, attrs, defStyleAttr);
+        this.mContext = context;
+        //在编辑模式下不需要进行初始化
+        if (!this.isInEditMode()) {
+            //初始化文件相关
