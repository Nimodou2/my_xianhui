+                                    if (!jo2.get("selected").isJsonNull()){
+                                        selected=jo2.get("selected").getAsBoolean();
+                                    }
+                                }else if (jo2.has("disabled")){
+                                    if (!jo2.get("disabled").isJsonNull()){
+                                        disabled=jo2.get("disabled").getAsBoolean();
+                                    }
