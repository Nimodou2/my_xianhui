<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_marginTop="10px"
    android:layout_width="match_parent"
    android:layout_height="90px"

    >
    <RelativeLayout
        android:background="@drawable/zhushou_realitive_day1"
        android:layout_marginLeft="25px"
        android:layout_marginRight="25px"
        android:layout_width="325px"
        android:layout_marginTop="10px"
        android:layout_centerInParent="true"
        android:id="@+id/zhushou_item_realitive_view"
        android:layout_height="70px">
        <LinearLayout
            android:layout_width="50px"
            android:layout_height="50px"
            android:layout_marginTop="10px"
            android:layout_marginLeft="15px"
            android:id="@+id/zhushou_item_linearlayout_view"
            android:orientation="vertical"
            android:background="@drawable/zhushou_lenerlayout_circleblack">
            <TextView
                android:layout_width="match_parent"
                android:textSize="14px"
                android:id="@+id/zhushou_item_week"
                android:textColor="#ffffff"
                android:text="周一"
                android:background="@drawable/zhushou_textviewbg_circle1"
                android:gravity="center"
                android:layout_height="22px"
                />
            <TextView
                android:layout_width="match_parent"
                android:textSize="24px"
                android:textColor="@color/bar_bg"
                android:id="@+id/zhushou_item_text_number"
                android:gravity="center"
                android:background="@drawable/zhushou_textview_bottom_circle"
                android:text="22"
                android:layout_height="28px"
                />
        </LinearLayout>
        <TextView
            android:layout_width="69px"
            android:textSize="22px"
            android:text="日报表"
            android:layout_marginTop="10px"
            android:id="@+id/zhushou_item_text_subject"
            android:layout_marginLeft="72px"
            android:textColor="#ffffff"
            android:gravity="center_vertical"
            android:layout_height="22px"
            />
        <TextView
            android:layout_width="69px"
            android:textSize="16px"
            android:text="hui"
            android:layout_marginTop="43px"
            android:layout_marginLeft="75px"
            android:gravity="center_vertical"
            android:id="@+id/zhushou_item_text_org_name"
            android:textColor="#ffffff"
            android:layout_height="17px"
            />
        <ImageView
            android:layout_width="12px"
            android:layout_height="24px"
            android:layout_marginLeft="298px"
            android:background="@mipmap/dailychartarrow3"
            android:layout_marginTop="23px"/>
    </RelativeLayout>
</RelativeLayout>